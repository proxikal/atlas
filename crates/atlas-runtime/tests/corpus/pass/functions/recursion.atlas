fn sum_to(n: number) -> number {
    if (n <= 0) {
        return 0;
    }
    return n + sum_to(n - 1);
}

fn power(base: number, exp: number) -> number {
    if (exp == 0) {
        return 1;
    }
    return base * power(base, exp - 1);
}

print(sum_to(5));
print(sum_to(10));
print(power(2, 8));
print(power(3, 4));
